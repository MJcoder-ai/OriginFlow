# Example environment configuration for OriginFlow
OPENAI_API_KEY=sk-...
DATABASE_URL=sqlite+aiosqlite:///./originflow.db
VITE_API_URL=http://localhost:8000/api/v1
# Flags controlling datasheet parsing strategies
USE_RULE_BASED=true
USE_TABLE_EXTRACTION=true
USE_AI_EXTRACTION=true
USE_OCR_FALLBACK=false
# Vector store configuration
QDRANT_HOST=host.docker.internal
QDRANT_COLLECTION=ai_action_vectors
VECTOR_SIZE=384
# Confidence-driven autonomy (default OFF). When false, all actions require approval.
AI_AUTO_APPROVE=false
