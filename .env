# Example environment variables for OriginFlow
# Copy this file to .env and fill in your local values.

# Backend Configuration

# DATABASE_URL="postgresql://user:password@localhost:5432/originflow"
DATABASE_URL="sqlite:///./originflow.db"

TEMPORAL_HOST="localhost:7233"

# External API Keys (replace with actual keys)
API_KEY="your_octopart_api_key_here"

# Emergency Shutdown Token (replace with a secure, generated token)
EMERGENCY_TOKEN="your_emergency_shutdown_bearer_token"

# For local development, can be set to DEBUG, INFO, WARNING, ERROR
LOG_LEVEL="DEBUG"
